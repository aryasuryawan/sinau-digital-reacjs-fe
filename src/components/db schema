CREATE TABLE `course` (
  `id` INT NOT NULL AUTO_INCREMENT,
  `title` VARCHAR(75) NOT NULL,
  `desc` VARCHAR (250) NULL,
  `price` INT(10) NULL,
  `content` TEXT NULL,
  `category` VARCHAR(40),
  `image` VARCHAR(250) ,
  `startAt` TIMESTAMP ,
  `finishAt` TIMESTAMP ,
  `createdAt` TIMESTAMP ,
  `updatedAt` TIMESTAMP ,
  `isDeleted` INT(1) not null,
  PRIMARY KEY (id)
);

